@{
	ViewData["Title"] = "Sản phẩm theo loại";
	Layout = "~/Views/Shared/_LayoutOgani.cshtml";
}

@using X.PagedList.Mvc.Core;
@model X.PagedList.IPagedList<TDanhMucSp>

@foreach (var item in Model)
{
	@await Html.PartialAsync("PartialSanPham", item);
}
<div class="product__pagination">
    @Html.PagedListPager(Model, page=>Url.Action("SanPhamTheoLoai", new{maloai=ViewBag.maloai, page=page}))
</div>



